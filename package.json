{
  "name": "sanscomicsans",
  "version": "1.0.0",
  "description": "An attempt to hide instances of Comic Sans in the world via image obfuscation. ",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test-redux": "mocha './test/reducers/**.js' './test/actions/**.js' --watch --require babel-register",
    "test-react": "mocha './test/components/**.js' --watch --require babel-register",
    "test-lib": "mocha './test/lib/**.js' --watch --require babel-register",
    "lint": "eslint src/* test/*"
  },
  "author": "grittly",
  "license": "ISC",
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "animejs": "^2.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.19",
    "clipboard": "^2.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "konva": "^2.0.2",
    "node-sass-loader": "^0.1.7",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-hot-loader": "^4.0.1",
    "react-icons": "^2.2.7",
    "react-konva": "^1.7.2",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.7",
    "seedrandom": "^2.4.3",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.0.5",
    "redux-devtools": "^3.4.1",
    "redux-devtools-extension": "^2.13.2",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  }
}
